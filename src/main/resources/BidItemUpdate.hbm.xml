<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="com.navprayas.bidding.engine.common.BidItemUpdate" table="autobids">
      <id name="id" column="id" type="java.lang.Long">
      </id>
        <property column="BIDITEMID" name="bidItemId" type="java.lang.Long"/>
        <property column="BIDSTARTTIME" name="bidStartTime" type="java.util.Date"/>
		<property column="BIDENDTIME" name="bidEndTime" type="java.util.Date"/>
		<property column="STATUSCODE" name="statusCode" type="java.lang.String"/>
		<property column="LASTUPDATETIME" name="lastUpdateTime" type="java.util.Date"/>
   </class>
</hibernate-mapping>